#include <stdio.h>

int main()
{
    int year,leapyear;
    printf("enter the year:\n");
    scanf("%d",&year);
    if(year%4==0 && year%100!=0)
    printf("the entered year is leapyear\n");
     else if(year%4==0 && year%100==0 && year%400==0)
    printf("the entered year is leapyear\n");
    else
    printf("the entered year is not a leapyear\n");
    

    return 0;
}
