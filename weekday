#include<stdio.h>
#include<math.h>
int main()
{
    int dd,mm,yy,y,h,q,m;
    printf("enter the date :\n");
    scanf("%d-%d-%d",&dd,&mm,&yy);
 if(mm==1)
 m==13;
 y=yy-1;
 if(mm=2)
 m==14;
 y=yy-1;
 if
 m==mm;
 y==yy;
 h= (q+13*(m+1)/5+y+y/4-y/100+y/400)%7;
 switch(h)
 {
     case 1:printf("saturday\n");
            break;
     case 2:printf("sunday\n");       
            break;
    case 3:printf("monday\n");
            break;
     case 4:printf("tuesday\n");
            break;
    case 5:printf("wednesday\n");
            break;
    case 6:printf("thursday\n");
           break;
    case 7:printf("friday\n");
           break;
 }
 }
 
}
